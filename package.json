{
  "name": "katalyn-syntax-highlight",
  "displayName": "Katalyn Syntax Highlight",
  "description": "A syntax highlighter for the Katalyn programming language.",
  "version": "0.0.2",
  "repository": "https://github.com/Lartu/Katalyn-VSC-Highlight-Extension",
  "publisher": "Lartu",
  "engines": {
    "vscode": "^1.91.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "katalyn",
      "aliases": ["Katalyn", "katalyn"],
      "extensions": [".kat"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "katalyn",
      "scopeName": "source.katalyn",
      "path": "./syntaxes/katalyn.tmLanguage.json"
    }]
  }
}
